{
  "cordis": {
    "checkpoint": "startDate",
    "runs": [
      {
        "query": "cultural OR heritage",
        "download_attachments": true,
        "checkpoint_to_range": "5"
      },
      {
        "query": "contenttype='project' AND '*'",
        "download_attachments": false,
        "checkpoint_to_range": "1"
      }
    ]
  },
  "coreac": {
    "checkpoint": "publishedDate",
    "runs": [
      {
        "query": "((computing AND cultural) OR (computing AND heritage))",
        "download_attachments": false,
        "checkpoint_to_range": "1"
      }
    ]
  },
  "arxiv": {
    "checkpoint": "start",
    "runs": [
      {
        "query": "all:computing+AND+(all:humanities+OR+all:heritage)",
        "download_attachments": true,
        "checkpoint_to_range": "100"
      }
    ]
  },
  "openaire": {
    "checkpoint": "pagination",
    "runs": [
      {
        "query": "cultural",
        "download_attachments": false,
        "checkpoint_to_range": "0"
      },
      {
        "query": "heritage",
        "download_attachments": false,
        "checkpoint_to_range": "0"
      }
    ]
  }
}